<%= form_tag products_path, method: :get do %>
<% ['Tops', 'Bottoms', 'Underwear', 'Outerwear', 'Shoes'].each do |category| %>
  <%= label_tag('category', category) %>
  <%= check_box_tag 'category', category, false, class: 'x' %>
  <br>
<% end %>
  <%= submit_tag "Search", class: "btn btn-primary" %>
<% end %>
